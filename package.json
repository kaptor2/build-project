    "babel-loader": "8.2.3",
    "html-webpack-plugin": "5.5.0",
    "react-refresh": "0.11.0",
    "typescript": "4.5.2",
    "webpack": "5.64.4",
    "webpack-cli": "4.9.1",
    "webpack-dev-server": "4.6.0",
    "webpack-merge": "5.8.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
